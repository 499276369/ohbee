/* Page styles for the Dataspace Viewer web application */

body {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	text-align: left;
	text-valign: top;
}

td {
	font-size: 11px;
}

h1 {
	font-size: 17px;
}

h2 {
	font-size: 14px;
}

h3 {
	font-size: 12px;
}

h4 {
	font-size: 12px;
}

a {
	text-decoration: none;
}

a:hover {
	text-decoration: underline;
}